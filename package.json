{
  "name": "novahub-obfuscator-service",
  "version": "1.0.0",
  "description": "NovaHub Obfuscator (API + Discord bot)",
  "main": "web/server.js",

  "scripts": {
    "start": "concurrently \"npm run start:web\" \"npm run start:bot\"",

    "start:web": "concurrently \"node web/server.js\" \"node web/bypasser.js\"",
    "start:bot": "node bot.js",

    "dev": "concurrently \"npm run dev:web\" \"npm run dev:bot\"",

    "dev:web": "concurrently \"nodemon web/server.js\" \"nodemon web/bypasser.js\"",
    "dev:bot": "nodemon bot.js"
  },

  "author": "NovaHub",
  "license": "MIT",

  "dependencies": {
    "axios": "^1.5.0",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "discord.js": "^14.14.0"
  },

  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
